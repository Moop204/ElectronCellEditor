{
  "compilerOptions": {
    "target": "ES2018",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"],
      "@back": ["backend/*"]
    },
    "jsx": "react", 


  },

    "include": [
    "src/**/*"
  ]
}
